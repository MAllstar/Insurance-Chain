  "source": "pragma solidity ^0.4.24;\r\n\r\ncontract Application {\r\n    //保存申请者的地址\r\n    address public applicant;\r\n\r\n    //申请理赔\r\n    function apply() public returns (address) {\r\n        //保存调用这地址\r\n        applicant = msg.sender;\r\n        return applicant;\r\n    }\r\n}",
